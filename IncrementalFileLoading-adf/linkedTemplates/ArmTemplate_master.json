{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "IncrementalFileLoading-adf"
		},
		"LS_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage'"
		},
		"ls_AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorage2'"
		},
		"sinkLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sinkLS'"
		},
		"sourceLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sourceLS'"
		},
		"LS_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://increfileloadingacc.dfs.core.windows.net/"
		},
		"ls_AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://increfileloadingacc.dfs.core.windows.net/"
		},
		"sinkLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://increfileloadingacc.dfs.core.windows.net/"
		},
		"sourceLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://increfileloadingacc.dfs.core.windows.net/"
		},
		"Trigger_ntm_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"Trigger_ntm_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "IncrementalFileLoading-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage_accountKey')]"
					},
					"ls_AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorage2_accountKey')]"
					},
					"sinkLS_accountKey": {
						"value": "[parameters('sinkLS_accountKey')]"
					},
					"sourceLS_accountKey": {
						"value": "[parameters('sourceLS_accountKey')]"
					},
					"LS_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"ls_AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"sinkLS_properties_typeProperties_url": {
						"value": "[parameters('sinkLS_properties_typeProperties_url')]"
					},
					"sourceLS_properties_typeProperties_url": {
						"value": "[parameters('sourceLS_properties_typeProperties_url')]"
					},
					"Trigger_ntm_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_ntm_properties_pipeline_parameters_windowStart')]"
					},
					"Trigger_ntm_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('Trigger_ntm_properties_pipeline_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}